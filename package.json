{
	"name": "starter-block-theme",
	"version": "0.1.0",
	"description": "Example block scaffolded with Create Block tool.",
	"author": "The WordPress Contributors",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"build": "wp-scripts build --webpack-src-dir=src --output-path=blocks ",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "wp-scripts start --webpack-src-dir=src --output-path=blocks",
		"compile-assets": "webpack --config webpack/webpack.config.main.js --watch"
	},
	"files": [
		"assets",
		"blocks",
		"includes",
		"parts",
		"patterns",
		"styles",
		"templates",
		"functions.php",
		"style.css",
		"screenshot.png",
		"theme.json"
	],
	"devDependencies": {
		"@wordpress/scripts": "^30.20.0"
	},
	"dependencies": {
		"@wordpress/icons": "^10.27.0"
	},
	"keywords": [],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/raphael-borges/starter-block-theme.git"
	},
	"bugs": {
		"url": "https://github.com/raphael-borges/starter-block-theme/issues"
	},
	"homepage": "https://github.com/raphael-borges/starter-block-theme#readme"
}
