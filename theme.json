{
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": false,
			"customDuotone": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Background",
					"slug": "background"
				},
				{
					"color": "#191836",
					"name": "Foreground",
					"slug": "foreground"
				},
				{
					"color": "#f59c00",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#e40134",
					"name": "Secondary",
					"slug": "secondary"
				}
			]
		},
		"custom": {
			"blockGap": "1.5rem"
		},
		"layout": {
			"contentSize": "650px",
			"wideSize": "1200px"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "2X-Small",
					"size": "clamp(0.56rem, calc(0.54rem + 0.11vw), 0.63rem)",
					"slug": "20"
				},
				{
					"name": "X-Small",
					"size": "clamp(0.88rem, 0.85rem + 0.11vi, 0.94rem)",
					"slug": "30"
				},
				{
					"name": "Small",
					"size": "clamp(1.13rem, calc(1.08rem + 0.22vw), 1.25rem)",
					"slug": "40"
				},
				{
					"name": "Medium",
					"size": "clamp(1.69rem, calc(1.62rem + 0.33vw), 1.88rem)",
					"slug": "50"
				},
				{
					"name": "Large",
					"size": "clamp(2.25rem, calc(2.16rem + 0.43vw), 2.50rem)",
					"slug": "60"
				},
				{
					"name": "X-Large",
					"size": "clamp(3.38rem, calc(3.24rem + 0.65vw), 3.75rem)",
					"slug": "70"
				},
				{
					"name": "2X-Large",
					"size": "clamp(4.5rem, 4.33rem + 0.87vi, 5rem)",
					"slug": "80"
				}
			]
		},
		"typography": {
			"customFontSize": true,
			"defaultFontSizes": false,
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/poppins-v21-latin-regular.woff2"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/poppins-v21-latin-700.woff2"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/poppins-v21-latin-300italic.woff2"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/poppins-v21-latin-700italic.woff2"
							]
						}
					],
					"fontFamily": "\"Poppins\", sans-serif",
					"name": "Poppins",
					"slug": "poppins"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "1em",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "1.2em",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "2em",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "2.4em",
					"slug": "x-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"button": {
				":active": {
					"color": {
						"background": "var(--wp--preset--color--background)",
						"text": "var(--wp--preset--color--secondary)"
					}
				},
				":focus": {
					"border": {
						"color": "var(--wp--preset--color--background)"
					},
					"color": {
						"background": "var(--wp--preset--color--secondary)",
						"text": "var(--wp--preset--color--background)"
					}
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--secondary)",
						"text": "var(--wp--preset--color--background)"
					}
				},
				":visited": {
					"color": {
						"text": "var(--wp--preset--color--background)"
					}
				},
				"border": {
					"color": "var(--wp--preset--color--background)",
					"radius": "0px",
					"width": "2px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--background)"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--20)",
						"left": "var(--wp--preset--spacing--50)",
						"right": "var(--wp--preset--spacing--50)",
						"top": "var(--wp--preset--spacing--20)"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-1)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-2)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-3)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-4)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-5)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-6)"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "400",
					"lineHeight": "1.2"
				}
			},
			"link": {
				":active": {
					"color": {
						"text": "var(--wp--preset--color--foreground)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "underline dashed"
					}
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--foreground)"
					},
					"typography": {
						"textDecoration": "underline"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"textDecoration": "underline"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--custom--block-gap)",
			"padding": {
				"bottom": "var(--wp--preset--spacing--30)",
				"left": "var(--wp--preset--spacing--20)",
				"right": "var(--wp--preset--spacing--20)",
				"top": "var(--wp--preset--spacing--30)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--poppins)",
			"fontSize": "var(--wp--preset--font-size--paragraph)",
			"lineHeight": "1.6"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer Credit Area"
		},
		{
			"area": "uncategorized",
			"name": "comments",
			"title": "Comments"
		},
		{
			"area": "uncategorized",
			"name": "post-meta",
			"title": "Post Meta"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.6/theme.json"
}